{
    "name": "pogotracker",
    "devDependencies": {
        "clean-css": "^5.3.3",
        "clean-css-cli": "^5.6.3",
        "eslint": "^9.39.2",
        "npm-run-all": "^4.1.5",
        "terser": "^5.44.1",
        "npm-check-updates": "19.3.1"
    },
    "type": "module",
    "scripts": {
        "lint": "eslint .",
        "minJS": "run-p minAdmin minBlog minHome minLogin minPokedex minPokemon minStats minTrade minTrainer minAlert minContact minCookie minCopy minDisplay minFetch minForm minLoading minModals minMultiselect minSocket minSearch minToast minGlobal minRuntime",
        "minCSS": "run-p cleanAdmin cleanBlog cleanGlobal cleanPokedex cleanTrade cleanTrainer cleanMultiselect",
        "minAdmin": "terser js/handlers/admin.js --config-file .terser.json --output build/js/handlers/admin.min.js",
        "minBlog": "terser js/handlers/blog.js --config-file .terser.json --output build/js/handlers/blog.min.js",
        "minHome": "terser js/handlers/home.js --config-file .terser.json --output build/js/handlers/home.min.js",
        "minLogin": "terser js/handlers/login.js --config-file .terser.json --output build/js/handlers/login.min.js",
        "minPokedex": "terser js/handlers/pokedex.js --config-file .terser.json --output build/js/handlers/pokedex.min.js",
        "minPokemon": "terser js/handlers/pokemon.js --config-file .terser.json --output build/js/handlers/pokemon.min.js",
        "minStats": "terser js/handlers/stats.js --config-file .terser.json --output build/js/handlers/stats.min.js",
        "minTrade": "terser js/handlers/trade.js --config-file .terser.json --output build/js/handlers/trade.min.js",
        "minTrainer": "terser js/handlers/trainer.js --config-file .terser.json --output build/js/handlers/trainer.min.js",
        "minAlert": "terser js/modules/alert.js --config-file .terser.json --output build/js/modules/alert.min.js",
        "minContact": "terser js/modules/contact.js --config-file .terser.json --output build/js/modules/contact.min.js",
        "minCookie": "terser js/modules/cookie.js --config-file .terser.json --output build/js/modules/cookie.min.js",
        "minCopy": "terser js/modules/copy.js --config-file .terser.json --output build/js/modules/copy.min.js",
        "minDisplay": "terser js/modules/display.js --config-file .terser.json --output build/js/modules/display.min.js",
        "minFetch": "terser js/modules/fetch.js --config-file .terser.json --output build/js/modules/fetch.min.js",
        "minForm": "terser js/modules/form.js --config-file .terser.json --output build/js/modules/form.min.js",
        "minLoading": "terser js/modules/loading.js --config-file .terser.json --output build/js/modules/loading.min.js",
        "minModals": "terser js/modules/modals.js --config-file .terser.json --output build/js/modules/modals.min.js",
        "minMultiselect": "terser js/modules/multiselect.js --config-file .terser.json --output build/js/modules/multiselect.min.js",
        "minSocket": "terser js/modules/socket.js --config-file .terser.json --output build/js/modules/socket.min.js",
        "minSearch": "terser js/modules/search.js --config-file .terser.json --output build/js/modules/search.min.js",
        "minToast": "terser js/modules/toast.js --config-file .terser.json --output build/js/modules/toast.min.js",
        "minGlobal": "terser js/global.js --config-file .terser.json --output build/js/global.min.js",
        "minRuntime": "terser js/runtime.js --config-file .terser.json --output build/js/runtime.min.js",
        "cleanAdmin": "cleancss -o build/css/admin.min.css css/admin.css",
        "cleanBlog": "cleancss -o build/css/blog.min.css css/blog.css",
        "cleanGlobal": "cleancss -o build/css/global.min.css css/global.css",
        "cleanPokedex": "cleancss -o build/css/pokedex.min.css css/pokedex.css",
        "cleanTrade": "cleancss -o build/css/trade.min.css css/trade.css",
        "cleanTrainer": "cleancss -o build/css/trainer.min.css css/trainer.css",
        "cleanMultiselect": "cleancss -o build/css/lib/multiselect.min.css css/lib/multiselect.css"
    }
}
