{
    "name":"pogotracker",
    "version":"1.0.0",
    "author":"lanechase34",
    "installPaths":{
        "coldbox":"app/coldbox/",
        "testbox":"app/modules/testbox/",
        "cbvalidation":"app/modules/cbvalidation/",
        "cbjavaloader":"app/modules/cbjavaloader/",
        "cbmailservices":"app/modules/cbmailservices/",
        "route-visualizer":"app/modules/route-visualizer/",
        "cbdebugger":"app/modules/cbdebugger/",
        "cfmigrations":"app/modules/cfmigrations/",
        "qb":"app/modules/qb/",
        "socketbox":"app/modules/socketbox/",
        "lucee-password-util":"app/modules/lucee-password-util/"
    },
    "devDependencies":{
        "testbox":"6.4.0+17",
        "testbox-cli":"1.6.0+23",
        "route-visualizer":"2.2.0+2",
        "commandbox-cfformat":"0.21.0",
        "cfml-compiler":"1.0.6",
        "lucee-password-util":"1.0.3",
        "commandbox-githooks":"1.0.2",
        "cbdebugger":"4.4.0+6"
    },
    "dependencies":{
        "coldbox":"8.0.5+33",
        "cbvalidation":"4.8.0+30",
        "commandbox-dotenv":"2.4.0",
        "commandbox-cfconfig":"2.1.1",
        "cbjavaloader":"2.3.0+2",
        "cbmailservices":"2.9.0+2",
        "cfmigrations":"5.1.0",
        "commandbox-migrations":"5.3.0",
        "qb":"13.0.17",
        "socketbox":"2.0.12"
    },
    "scripts":{
        "format":"cfformat run Application.cfc,WebSocket.cfc,app/config/,app/handlers/,app/interceptors/,app/models/,tests/,app/database/ ./.cfformat.json --overwrite",
        "format:check":"cfformat check Application.cfc,WebSocket.cfc,app/config/,app/handlers/,app/interceptors/,app/models/,tests/,app/database/ ./.cfformat.json",
        "format:watch":"cfformat watch path='Application.cfc,WebSocket.cfc,app/config/,app/handlers/,app/interceptors/,app/models/,tests/,app/database/' settingsPath='.cfformat.json'",
        "docker:build":"!docker build --no-cache -t pogotracker -f ./Dockerfile ./",
        "setEnv":"cp env/${1}.dev.env .env",
        "blankEnv":"cp .env.example .env"
    },
    "testbox":{
        "runner":"http://localhost:8081/tests/runner.cfm"
    },
    "language":"cfml",
    "githooks":{
        "preCommit":[
            "run-script format",
            "dotenv check --reverse"
        ],
        "postCheckout":[
            "install",
            "dotenv check"
        ],
        "postMerge":[
            "dotenv check"
        ]
    }
}
